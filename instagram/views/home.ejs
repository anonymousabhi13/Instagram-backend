        <a id="comment" href="/comment"> <i id="like" class="ri-chat-3-line"></i></a>
        <i id="like" class="ri-share-forward-line"></i>
        <i id="like" class="ri-bookmark-line"></i>
      </div>
      <h4 class="likebadhao">
        <%= show.comment%> 
      </h4>
      <h4 class="likebadhao">
        <%= show.like.length %> likes
      </h4>
      <h4 style="margin-left: 18px;" id="account"><b>
